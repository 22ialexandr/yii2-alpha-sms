{
	"name": "richweber/yii2-alpha-sms",
	"version":"1.0.0",
	"description": "AlphaSMS API for the Yii framework",
	"keywords": ["yii2", "aplhasms", "sms", "api"],
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "",
		"wiki": "",
		"source": ""
	},
	"authors": [
		{
			"name": "RichWeber",
			"email": "rbagatyi@gmail.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2-app-advanced": "*",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-swiftmailer": "*",
		"kartik-v/yii2-helpers": "dev-master",
		"kartik-v/yii2-icons": "dev-master",
		"asofter/yii2-imperavi-redactor": "*",
		"kartik-v/bootstrap-fileinput": "dev-master",
		"2amigos/yii2-file-input-widget" : "*",
		"yiisoft/yii2-sphinx": "*"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*",
		"codeception/specify": "*",
		"codeception/verify": "*"
	},
	"autoload": {
		"psr-4": { "yii\\aplha\\sms\\": "" }
	},
	"suggest": {
		"codeception/codeception": "Codeception, 1.8.*@dev is currently works well with Yii.",
		"codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
		"codeception/verify": "BDD Assertions for PHPUnit and Codeception",
		"yiisoft/yii2-faker": "Fixtures generator for Yii2 based on Faker lib"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"backend/runtime",
			"backend/web/assets",

			"frontend/runtime",
			"frontend/web/assets"
		]
	}
}